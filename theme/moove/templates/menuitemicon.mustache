{{!
    @template theme_moove/menuitemicon

    Displays the admin tree as a list of grouped links, just the children part.

    Example context (json):
    {
        "node": {
            "text": "Root of menu",
            "key": "test0",
            "action": "https://mymoodle.org/action",
            "display": true,
            "children": [
                {
                    "text": "Child of menu",
                    "key": "test1",
                    "action": "https://mymoodle.org/action",
                    "display": true,
                    "children": []
                }
            ]
        }
    }
}}
{{#showmenuitemicons}}
{{#icon.pix}}
{{#pix}}{{icon.pix}}, {{icon.component}}{{/pix}}
{{/icon.pix}}
{{/showmenuitemicons}}
